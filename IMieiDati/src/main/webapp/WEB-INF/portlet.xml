  <portlet>
    <description>I_Miei_Dati_Portlet</description>
    <portlet-name>I_Miei_Dati_Portlet</portlet-name>
    <display-name>I_Miei_Dati_Portlet</display-name>
    
    <portlet-class>it.eng.eli4u.imieidati.IMieiDati</portlet-class>
    <expiration-cache>0</expiration-cache>
    <supports>
      <mime-type>text/html</mime-type>
      <portlet-mode>VIEW</portlet-mode>
      <portlet-mode>EDIT</portlet-mode>
      <portlet-mode>edit_defaults_compatibility</portlet-mode>
    </supports>
    <supported-locale>en</supported-locale>
    
    <portlet-info>
      <title>I_Miei_Dati_Portlet</title>
      <short-title>I_Miei_Dati_Portlet</short-title>
      <keywords>miei dati</keywords>
    </portlet-info>
    
    
      <portlet-preferences>
        <preference>
            <name>sectionsToHide</name>
            <value></value>
            <read-only>false</read-only>
        </preference>
  </portlet-preferences> 
  </portlet>    
  
