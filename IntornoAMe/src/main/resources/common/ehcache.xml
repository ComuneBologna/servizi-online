<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="true"
	monitoring="autodetect" dynamicConfig="true">

	<diskStore path="java.io.tmpdir/ehcache/intornoame" />
	
	<cache name="intornoAMeFrontCache" maxEntriesLocalHeap="1000"
		maxEntriesLocalDisk="10000" eternal="false" diskSpoolBufferSizeMB="20"
		timeToIdleSeconds="0" timeToLiveSeconds="600"
		memoryStoreEvictionPolicy="LFU" transactionalMode="off">
		<persistence strategy="localTempSwap" />
	</cache>
</ehcache>